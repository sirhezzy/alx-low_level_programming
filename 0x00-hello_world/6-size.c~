#include <stdio.h>
/**
 * main -  program that prints the size of various types on the computer
 * return (0);
 */
int main(void)
{
	char a;
	int b;
	long int c;
	long long int d;
	float f;

printf("Size of a char: %lu byte(s)\n", (unsigned long)size of(a));
printf("Size of an int: %lu byte(s)\n", (unsigned long)size of(a));
printf("Size of a long int: %lu byte(s)\n", (unsigned long)size of(a));
printf("Size of a long long int: %lu byte(s)\n", (unsigned long)size of(a));
printf("Size of a float: %lu byte(s)\n", (unsigned long)size of(a));
return (0);
}
